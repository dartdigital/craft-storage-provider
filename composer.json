{
  "name": "dart-digital/craft-storage-provider",
  "description": "Library for Craft Filesystems",
  "type": "craft-plugin",
  "license": "proprietary",
  "keywords": ["Craft CMS", "Filesystem", "StorageProvider", "Flysystem"],
  "homepage": "https://d-art-digital.de",
  "authors": [
    {
      "name": "Dart Digital",
      "homepage": "https://d-art-digital.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Dart\\Library\\Craft\\StorageProvider\\": "src/"
    }
  },
  "require": {
    "ext-bcmath": "*",
    "ext-intl": "*",
    "ext-zip": "*",
    "craftcms/cms": "^5.3.0",
    "craftcms/flysystem": "^2.0",
    "league/flysystem": "^3.0",
    "league/flysystem-aws-s3-v3": "^3.0",
    "monolog/monolog": "^3.8",
    "phpstan/phpstan": "^2.0",
    "symfony/dotenv": "^7.2",
    "symfony/phpunit-bridge": "^7.2"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "name": "Craft Storage Provider",
    "handle": "craft-storage-provider"
  },
  "require-dev": {
    "craftcms/generator": "^2.1"
  }
}
